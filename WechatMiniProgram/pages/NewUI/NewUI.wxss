page {
  background-color: initial;
  height: 100%;
}
.background {
  background-size: 100% 100%;
  height: 100%;
  left: 0;
  position: absolute;
  width: 100%;
  z-index: -2;
}
.DisINF {
  font-size: 15px;
  text-align: right;
}
.DisCr,
.DisINF {
  background-color: #88888850;
  color: rgba(11, 255, 133, 0.788);
}
.DisCr {
  font-size: 13px;
  text-align: center;
}
.DisTIP {
  background-color: #88888850;
  color: hsla(0, 0%, 100%, 0.658);
  text-align: left;
}
.DisINF1 {
  color: rgba(11, 255, 133, 0.788);
  text-align: right;
  width: 30%;
}
.DisINF1,
.DisTIP1 {
  background-color: #88888850;
  font-size: 15px;
  height: 35;
}
.DisTIP1 {
  color: rgba(0, 183, 255, 0.836);
  text-align: left;
  width: 70%;
}
.bterr {
  background-color: #0bce2c;
  border: 1px solid #666665;
  border-radius: 0;
  color: #121312;
  font-size: 28rpx;
  height: 22px;
  width: 18%;
}
.btnLayout {
  text-align: center;
}
.CrLayout,
.SendLayout,
.btnLayout {
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin: 0;
}
.CrLayout {
  text-align: center;
}
.bt1,
.bt2,
.bt3 {
  display: -webkit-flex;
  display: flex;
  height: 100rpx;
  width: 100rpx;
}
.SendBox {
  border: 1px solid #000;
}
.conic-progress {
  border-radius: 50%;
  height: 300rpx;
  margin: 30rpx auto;
  position: relative;
  width: 300rpx;
}
.conic-progress::before {
  background-color: #000;
  border-radius: 50%;
  content: "";
  height: 80%;
  width: 80%;
}
.conic-progress::before,
.progress-value {
  left: 50%;
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
}
.progress-value {
  color: green;
  font-size: 60rpx;
  font-weight: bolder;
}
.red {
  background-color: red;
  border-color: red;
  color: #fff;
}
view button {
  background-color: #88888850;
  border-color: #5550;
  border-radius: 0;
}
.tip2 {
  left: 4%;
  position: relative;
  top: 5px;
  width: 94%;
}
