.dialog {
  opacity: 0;
  transition: all 0.3s;
  visibility: hidden;
}
.dialog_show {
  opacity: 1;
  visibility: visible;
}
.target {
  background-image: url(https://cdn.jsdelivr.net/gh/makergyt/mini-color-picker@v0.1.2/screenshot/color_picker_mask.png);
  border: 0.5px solid rgba(0, 0, 0, 0.5);
  height: 600rpx;
  margin: 0 auto;
  overflow: hidden;
  width: 600rpx;
}
.ribbon {
  background: -webkit-linear-gradient(
    left,
    red,
    #ff0 17%,
    #0f0 33%,
    #0ff 50%,
    #00f 67%,
    #f0f 83%,
    red
  );
  margin: 40rpx auto;
  width: 600rpx;
}
@font-face {
  font-family: iconfont;
  font-style: normal;
  font-weight: 400;
  src: url(data:font/truetype;charset=utf-8;base64,)
    format("truetype");
}
.iconfont {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color: #fff;
  font-family: iconfont !important;
  font-size: 28px;
  font-style: normal;
  height: 28px;
  line-height: 28px;
  text-align: center;
  width: 28px;
}
.icon-ios-locate-outline:before {
  content: "î©‰";
}
.weui-mask {
  background: rgba(0, 0, 0, 0.6);
  right: 0;
  top: 0;
  z-index: 1000;
}
.weui-actionsheet,
.weui-mask {
  bottom: 0;
  left: 0;
  position: fixed;
}
.weui-actionsheet {
  backface-visibility: hidden;
  background-color: #fff;
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
  overflow: hidden;
  padding-bottom: calc(env(safe-area-inset-bottom));
  transform: translate(0, 100%);
  transition: -webkit-transform 0.3s;
  transition: transform 0.3s;
  transition: transform 0.3s, -webkit-transform 0.3s;
  width: 100%;
  z-index: 5000;
}
.weui-actionsheet_toggle {
  transform: translate(0, 0);
}
.weui-half-screen-dialog__hd {
  -webkit-box-align: center;
  align-items: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 8px;
  height: 8em;
  padding: 0 calc(24px + env(safe-area-inset-right));
}
.weui-half-screen-dialog__hd .weui-icon-btn {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.weui-half-screen-dialog__hd__side {
  left: -8px;
  position: relative;
}
.weui-half-screen-dialog__hd__main {
  -webkit-box-flex: 1;
  flex: 1;
}
.weui-half-screen-dialog__hd__side + .weui-half-screen-dialog__hd__main {
  padding: 0 40px;
  text-align: center;
}
.weui-half-screen-dialog__hd__main + .weui-half-screen-dialog__hd__side {
  left: auto;
  right: -8px;
}
.weui-half-screen-dialog__hd__main
  + .weui-half-screen-dialog__hd__side
  .weui-icon-btn {
  right: 0;
}
.weui-half-screen-dialog__title {
  color: rgba(0, 0, 0, 0.9);
  display: block;
  font-size: 15px;
  font-weight: 700;
}
.weui-half-screen-dialog__subtitle {
  color: rgba(0, 0, 0, 0.5);
  display: block;
  font-size: 10px;
}
.weui-icon-close-thin {
  background-color: currentColor;
  display: inline-block;
  height: 24px;
  mask-image: url(data:image/svg+xml;charset=utf-8,%3Csvg\ width=\'24\'\ height=\'24\'\ xmlns=\'http://www.w3.org/2000/svg\'%3E%3Cpath\ d=\'M12.25\ 10.693\ 6.057\ 4.5\ 5\ 5.557l6.193\ 6.193L5\ 17.943\ 6.057\ 19l6.193-6.193L18.443\ 19l1.057-1.057-6.193-6.193L19.5\ 5.557\ 18.443\ 4.5z\'\ fill-rule=\'evenodd\'/%3E%3C/svg%3E);
  mask-position: 50% 50%;
  mask-repeat: no-repeat;
  mask-size: 100%;
  vertical-align: middle;
  width: 24px;
}
